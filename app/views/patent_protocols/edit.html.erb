<h1>Editing patent_protocol</h1>

<% form_for(@patent_protocol) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :time %><br />
    <%= f.date_select :time %>
  </p>
  <p>
    <%= f.label :comment%><br />
    <%= f.text_field :comment%>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @patent_protocol %> |
<%= link_to 'Back', patent_protocols_path %>
